@model ViewNotesViewModel
@{
    Layout = "_AdminDashboardLayout";
    ViewData["UserName"] = Model.UserName;
}

@* MODALS *@
<div class="modal fade" id="transferNotesModal" aria-labelledby="exampleModalCenterTitle">
    <div class="modal-dialog rounded-3 modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header bg-info">
                <h5 class="modal-title text-white" id="exampleModalLongTitle">Transfer Notes</h5>
                <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="m-4 modal-body align-items-center  mt-4">
                @if (Model.requeststatuslogs != null && Model.requeststatuslogs.Any())
                {
                    @foreach (var log in Model.requeststatuslogs)
                    {
                        @if (log.Notes != null)
                        {
                            <div class="d-flex">
                                <div class=" border-1 rounded-circle  mt-2" id="" style="width: 10px; height:10px; background-color:black;">
                                </div>
                                <div class="ms-2 fw-normal" style="color:grey;">
                                    @log.Notes
                                </div>
                            </div>
                        }
                    }
                }
                else
                {
                    <div class="m-0 fw-normal" style="color:grey;">
                        No transfer notes yet
                    </div>
                }

            </div>

            <div class="modal-footer">

                <button type="button" class="btn cancel-btn" data-bs-dismiss="modal">Cancel</button>
            </div>

        </div>
    </div>
</div>


