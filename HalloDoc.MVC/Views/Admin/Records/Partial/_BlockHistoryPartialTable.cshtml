@model PagedList<BlockedHistory><div class="mt-3 table-responsive">    <table class="table laptop">        <colgroup>            <col span="1" style="width: 15%;">            <col span="1" style="width: 15%;">            <col span="1" style="width: 20%;">            <col span="1" style="width: 15%;">            <col span="1" style="width: 20%;">            <col span="1" style="width: 15%;">        </colgroup>        <thead class="table-head align-middle">            <tr>                <th>Patient Name</th>                <th>Phone Number</th>                <th>Created Date</th>                <th>Notes</th>                <th style="text-align:center;">Is Active</th>                <th class="justify-content-center">Action</th>            </tr>        </thead>        <tbody class="table-body align-middle">            @foreach (var record in Model)            {                <tr>                    <td>@record.PatientName</td>                    <td>@record.PhoneNumber</td>                    <td>@record.CreatedDate</td>                    <td>@record.Notes</td>                    <td style="text-align:center">                        <input type="checkbox" disabled style="height:15px;width:15px" @(record.IsActive == true ? "checked" : "") class="btn-outline-info" />                    </td>                    <td>                        <a asp-controller="Admin" asp-action="Unblock" asp-route-requestId="@record.RequestId" type="button" class="btn btn-outline-info">Unblock</a>                    </td>                </tr>            }        </tbody>    </table></div>