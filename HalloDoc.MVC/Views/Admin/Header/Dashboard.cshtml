@model AdminDashboardViewModel
@{
    Layout = "_AdminDashboardLayout";
}

@* MODALS DIV*@
<div id="modal-div"></div>

@* Request Support *@
<div class="modal fade" id="RequestSupportModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header bg-info">
                <div class="modal-dark modal-title text-white" id="exampleModalLongTitle">
                    Request Support
                </div>
                <button type="button" class="btn" aria-label="Close" data-bs-dismiss="modal">
                    <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 0 24 24" width="24px"
                         fill="#FFFFFF">
                        <path d="M0 0h24v24H0V0z" fill="none" />
                        <path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12 19 6.41z" />
                    </svg>
                </button>
            </div>

            <form method="post" asp-controller="Admin" asp-action="RequestDTY">

                <div class="modal-body">
                    <span class="modal-head">To all the unscheduled physicians, we are short on coverage and need additional support on call to respond to request.  </span>
                    <div class="modal-flex my-3">


                        <div class="form-floating mt-3">
                            <textarea type="text" class="form-control" name="message" placeholder="message" id="requestDTYdescription"></textarea>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button id="requestDTYsubmit" type="submit" class="btn btn-info text-white" data-bs-dismiss="modal">
                        Send
                    </button>
                    <button type="button" class="btn cancel-btn" data-bs-dismiss="modal">Cancel</button>
                </div>

            </form>
        </div>
    </div>
</div>

<div class="px-4 admin-dashboard">

